<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.su.lapponampai_w.mhm_app_beta1.TakeSkipMedicineActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#dabaac0f"
        android:id="@+id/linearLayout6">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView2"
                android:src="@drawable/light85x85"
                android:layout_margin="@dimen/z2_margin_space" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:text="New Text"
                android:id="@+id/textViewTSMHeadline"
                android:textSize="@dimen/heading3"
                android:gravity="center_vertical"
                android:layout_margin="@dimen/z2_margin_space"
                android:textColor="#d22e2e" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/linearLayout6"
        android:layout_centerHorizontal="true"
        android:id="@+id/linearLyout7">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageViewTSM"
                android:src="@drawable/ib0102"
                android:layout_margin="@dimen/z2_margin_space" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:text="New Text"
                android:id="@+id/textViewTSML1"
                android:gravity="center_vertical"
                android:textSize="@dimen/heading4" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/textViewTSML2"
                android:layout_marginLeft="20dp"
                android:textSize="@dimen/heading4" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="#d3a3a3">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/textView47"
            android:textSize="18sp"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginTop="@dimen/z2_margin_space"
            android:layout_marginBottom="@dimen/z2_margin_space" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text=" "
            android:id="@+id/textView90"
            android:background="#ffffff"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/textView48"
            android:textSize="18sp"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginTop="@dimen/z2_margin_space"
            android:layout_marginBottom="@dimen/z2_margin_space" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:text=" "
            android:id="@+id/textView91"
            android:background="#ffffff"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/textView49"
            android:textSize="18sp"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginTop="@dimen/z2_margin_space"
            android:layout_marginBottom="@dimen/z2_margin_space" />
    </LinearLayout>

</RelativeLayout>
